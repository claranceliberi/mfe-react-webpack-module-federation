{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React App",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "ESNext",
    "target": "es6"
  },
  "paths": {
    "~/*": ["./*"],
    "@/*": ["./src/*"]
  }
}
